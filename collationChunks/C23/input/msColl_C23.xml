   <surface lrx="5138" lry="7032" partOf="#ox-frankenstein_volume_ii" ulx="0" uly="0" folio="34v" shelfmark="MS. Abinger c. 57" base="ox-ms_abinger_c57/ox-ms_abinger_c57-0068.xml" id="ox-ms_abinger_c57-0068" sID="ox-ms_abinger_c57-0068"/>
